{
  "$id": "NumberValidation",
  "title": "Number validation",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "const": "number"
    },
    "minimum": {
      "type": "number",
      "default": 0
    },
    "maximum": {
      "type": "number",
      "default": 0
    },
    "enumOptions": {
      "type": "array",
      "items": {
        "type": "number",
        "minLength": 1
      },
      "default": [1, 2, 3, 4, 5, 6]
    }
  },
  "required": ["type", "minimum", "maximum"],
  "additionalProperties": false
}
