{
  "root": "libs/resource",
  "sourceRoot": "libs/resource/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/resource",
        "tsConfig": "libs/resource/tsconfig.lib.json",
        "packageJson": "libs/resource/package.json",
        "main": "libs/resource/src/index.ts",
        "assets": ["libs/resource/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/resource/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/resource"],
      "options": {
        "jestConfig": "libs/resource/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
