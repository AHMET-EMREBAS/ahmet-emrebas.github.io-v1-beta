{
  "root": "libs/commonjs",
  "sourceRoot": "libs/commonjs/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/commonjs",
        "main": "libs/commonjs/src/index.ts",
        "tsConfig": "libs/commonjs/tsconfig.lib.json",
        "assets": ["libs/commonjs/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/commonjs/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/commonjs"],
      "options": {
        "jestConfig": "libs/commonjs/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
