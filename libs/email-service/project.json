{
  "root": "libs/email-service",
  "sourceRoot": "libs/email-service/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/email-service",
        "tsConfig": "libs/email-service/tsconfig.lib.json",
        "packageJson": "libs/email-service/package.json",
        "main": "libs/email-service/src/index.ts",
        "assets": ["libs/email-service/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/email-service/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/email-service"],
      "options": {
        "jestConfig": "libs/email-service/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
