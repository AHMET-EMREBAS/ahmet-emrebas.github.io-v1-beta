{
  "root": "libs/dtos",
  "sourceRoot": "libs/dtos/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/dtos",
        "main": "libs/dtos/src/index.ts",
        "tsConfig": "libs/dtos/tsconfig.lib.json",
        "assets": ["libs/dtos/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/dtos/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/dtos"],
      "options": {
        "jestConfig": "libs/dtos/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
