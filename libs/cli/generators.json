{
  "$schema": "http://json-schema.org/schema",
  "name": "cli",
  "version": "0.0.1",
  "generators": {
    "cli": {
      "factory": "./src/generators/cli/generator",
      "schema": "./src/generators/cli/schema.json",
      "description": "cli generator"
    },
    "web": {
      "factory": "./src/generators/web/generator",
      "schema": "./src/generators/web/schema.json",
      "description": "web generator"
    },
    "api": {
      "factory": "./src/generators/api/generator",
      "schema": "./src/generators/api/schema.json",
      "description": "api generator"
    },
    "resource": {
      "factory": "./src/generators/resource/generator",
      "schema": "./src/generators/resource/schema.json",
      "description": "resource generator"
    },
    "crud": {
      "factory": "./src/generators/crud/generator",
      "schema": "./src/generators/crud/schema.json",
      "description": "crud generator"
    }
  }
}
