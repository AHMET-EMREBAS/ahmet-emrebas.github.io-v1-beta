{
  "projectType": "library",
  "root": "libs/pm",
  "sourceRoot": "libs/pm/src",
  "prefix": "ahmet-emrebas",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/libs/pm"],
      "options": {
        "project": "libs/pm/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/pm/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/pm/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/pm"],
      "options": {
        "jestConfig": "libs/pm/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/pm/src/**/*.ts", "libs/pm/src/**/*.html"]
      }
    }
  },
  "tags": []
}
