{
  "root": "libs/aegen",
  "sourceRoot": "libs/aegen/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/aegen",
        "main": "libs/aegen/src/index.ts",
        "tsConfig": "libs/aegen/tsconfig.lib.json",
        "assets": [
          "libs/aegen/*.md",
          {
            "input": "./libs/aegen/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/aegen/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/aegen",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/aegen",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/aegen/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/aegen"],
      "options": {
        "jestConfig": "libs/aegen/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
