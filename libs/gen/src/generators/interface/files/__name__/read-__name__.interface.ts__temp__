import { I<%- classname %> } from './<%- name %>.interface';

<% if(options.Relations?.length > 0 ) { %> 
export type  ICreate<%- classname %> = I<%- classname %><
<% for(const o of options.Relations || []) { %> 
     <% if(o.type.endsWith('One')) {%> 
      o.target,
    <% } else {  %> 
     <%- o.target %>[],
    <% } %>
<% } %>
> 

<% } else { %> 
export type  ICreate<%- classname %> = I<%- classname %>
<% } %>

